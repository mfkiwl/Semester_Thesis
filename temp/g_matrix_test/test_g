static float test_accx, test_accy, test_accz;
static float test_phi, test_theta, test_psi; 
static float g11, g12, g13, g21, g22, g23, g31, g32, g33;

PARAM_ADD(PARAM_FLOAT, test_phi, &test_phi)
PARAM_ADD(PARAM_FLOAT, test_theta, &test_theta)
PARAM_ADD(PARAM_FLOAT, test_psi, &test_psi)

LOG_ADD(LOG_FLOAT, test_accx, &test_accx)
LOG_ADD(LOG_FLOAT, test_accy, &test_accy)
LOG_ADD(LOG_FLOAT, test_accz, &test_accz)

LOG_ADD(LOG_FLOAT, g11, &g11)
LOG_ADD(LOG_FLOAT, g12, &g12)
LOG_ADD(LOG_FLOAT, g13, &g13)
LOG_ADD(LOG_FLOAT, g21, &g21)
LOG_ADD(LOG_FLOAT, g22, &g22)
LOG_ADD(LOG_FLOAT, g23, &g23)
LOG_ADD(LOG_FLOAT, g31, &g31)
LOG_ADD(LOG_FLOAT, g32, &g32)
LOG_ADD(LOG_FLOAT, g33, &g33)

// TODO: delete!
test_accx = g11*test_phi + g12*test_theta + g13*test_psi;
test_accy = g21*test_phi + g22*test_theta + g23*test_psi;
test_accz = g31*test_phi + g32*test_theta + g33*test_psi;
